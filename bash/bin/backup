#!/usr/bin/env bash
b2 sync --delete                          \
  --excludeAllSymlinks                    \
  --excludeDirRegex ^Documents            \
  --excludeDirRegex ^Downloads            \
  --excludeDirRegex ^go/                  \
  --excludeDirRegex ^Mail                 \
  --excludeDirRegex ^Music                \
  --excludeDirRegex ^src/                 \
  --excludeDirRegex ^\.cache              \
  --excludeDirRegex ^\.cargo              \
  --excludeDirRegex ^\.local/lib          \
  --excludeDirRegex ^\.local/share/nvim   \
  --excludeDirRegex ^\.local/src          \
  --excludeDirRegex ^\.password-store     \
  --excludeDirRegex ^\.rustup             \
  --excludeDirRegex ^\.vim                \
  ${HOME} b2://jhiggs-devbox-backup/
